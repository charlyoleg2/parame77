{
	"name": "desi77-ui",
	"description": "sveltekit-ui for desi77",
	"version": "0.1.4",
	"private": true,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/charlyoleg2/parame77.git"
	},
	"homepage": "https://charlyoleg2.github.io/parame77/",
	"author": "charlyoleg2",
	"license": "ISC",
	"keywords": [
		"parametrix",
		"3D",
		"CAD"
	],
	"type": "module",
	"scripts": {
		"cp_pgdsvg": "fse copy ../../node_modules/desi77/dist/pgdsvg static/pgdsvg",
		"prebuild": "svelte-kit sync",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"test:unit:once": "vitest --run",
		"lint": "prettier --check . && eslint .",
		"ci": "run-s cp_pgdsvg build lint check test:unit:once",
		"format": "prettier --write .",
		"github-pages-fix": "touch build/.nojekyll",
		"clean": "rimraf .svelte-kit build node_modules static/pgdsvg"
	},
	"dependencies": {
		"desi77": "^0.1.4",
		"geometrix": "^1.0.18",
		"geomui": "^1.0.18"
	},
	"devDependencies": {
		"@atao60/fse-cli": "^0.1.9",
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.20.7",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@types/eslint": "^9.6.1",
		"eslint": "^9.24.0",
		"eslint-config-prettier": "^10.1.2",
		"eslint-plugin-svelte": "^3.5.1",
		"globals": "^16.0.0",
		"npm-run-all2": "^7.0.2",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.3.3",
		"rimraf": "^6.0.1",
		"sass": "^1.86.3",
		"svelte": "^5.26.3",
		"svelte-check": "^4.1.6",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.30.1",
		"vite": "^6.2.6",
		"vitest": "^3.1.1"
	}
}
